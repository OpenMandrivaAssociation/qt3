+void QXIMInputContext::reset()
+{
+#if !defined(QT_NO_XIM)
+    if ( focusWidget() && isComposing() && ! composingText.isNull() ) {
+#ifdef QT_XIM_DEBUG
+	qDebug("QXIMInputContext::reset: composing - sending IMEnd (empty) to %p",
+	       focusWidget() );
+#endif // QT_XIM_DEBUG
+
+	QInputContext::reset();
+	resetClientState();
+
+	char *mb = XmbResetIC((XIC) ic);
+	if (mb)
+	    XFree(mb);
+    }
+#endif // !QT_NO_XIM
+}
