+void QXIMInputContext::sendIMEvent( QEvent::Type type, const QString &text,
+				    int cursorPosition, int selLength )
+{
+    QInputContext::sendIMEvent( type, text, cursorPosition, selLength );
+    if ( type == QEvent::IMCompose )
+	composingText = text;
