+
+void QXIMInputContext::resetClientState()
+{
+#if !defined(QT_NO_XIM)
+    composingText = QString::null;
+    if ( selectedChars.size() < 128 )
+	selectedChars.resize( 128 );
+    selectedChars.fill( 0 );
+#endif // !QT_NO_XIM
+}
